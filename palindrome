class Solution {
    public boolean isPalindrome(int x) {
        if (x >= 0) {
            int num = x;
            int val = 0;
            while (num > 0) {
                int rem = num % 10;
                val = val * 10 + rem;
                num /= 10;
            }
        if(val == x){
            return true;
        }
        }
        return false;
    }
}
